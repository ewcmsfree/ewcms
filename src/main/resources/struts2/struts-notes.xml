<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="notes" extends="struts-default" namespace="/notes">
		<action name="index" class="com.ewcms.content.notes.web.MemorandaAction" method="index">
			<result>/WEB-INF/page/notes/index.jsp</result>
		</action>
		<action name="delete" class="com.ewcms.content.notes.web.MemorandaAction" method="delete"/>
		<action name="input" class="com.ewcms.content.notes.web.MemorandaAction" method="input">
			<result name="input">/WEB-INF/page/notes/edit.jsp</result>
		</action>
		<action name="save" class="com.ewcms.content.notes.web.MemorandaAction" method="save">
			<result>/WEB-INF/page/notes/edit.jsp</result>
			<result name="input">/WEB-INF/page/notes/edit.jsp</result>
		</action>
		<action name="changeDate" class="com.ewcms.content.notes.web.MemorandaAction" method="changeDate">
			<result>/WEB-INF/page/notes/index.jsp</result>
		</action>
		<action name="toDay" class="com.ewcms.content.notes.web.MemorandaAction" method="toDay"/>
		<action name="drop" class="com.ewcms.content.notes.web.MemorandaAction" method="drop"/>
		<action name="notesRemind" class="com.ewcms.content.notes.web.MemorandaAction" method="notesRemind"/>
		<action name="list">
			<result>/WEB-INF/page/notes/list.jsp</result>
		</action>
		<action name="query" class="com.ewcms.content.notes.web.MemorandaQueryAction" method="query"/>
	</package>
</struts>	