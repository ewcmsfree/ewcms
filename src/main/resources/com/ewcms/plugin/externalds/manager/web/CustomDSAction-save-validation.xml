<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
"http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">

<validators>
	<field name="customDSVo.name">
		<field-validator type="requiredstring">
			<message>必须输入名称</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">20</param>
			<message>名称不能大于${maxLength}个字符</message>
		</field-validator>
	</field>
	<field name="customDSVo.customName">
		<field-validator type="requiredstring">
			<message>必须输入Custom名称</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">100</param>
			<message>Custom名称不能大于${maxLength}个字符</message>
		</field-validator>
	</field>
	<field name="customDSVo.customMethod">
		<field-validator type="requiredstring">
			<message>必须输入Custom方法</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">100</param>
			<message>Custom方法不能大于${maxLength}字符</message>
		</field-validator>
	</field>
</validators>