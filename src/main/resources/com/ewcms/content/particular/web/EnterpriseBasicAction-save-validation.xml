<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
"http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">

<validators>
    <field name="enterpriseBasicVo.name">
        <field-validator type="requiredstring">
            <message>不能为空</message>
        </field-validator>
        <field-validator type="stringlength">
        	<param name="maxLength">200</param>
        	<param name="trim">true</param>
        	<message>必须小于200个字</message>
        </field-validator>
    </field>
    <field name="enterpriseBasicVo.yyzzzch">
        <field-validator type="requiredstring">
            <message>不能为空</message>
        </field-validator>
        <field-validator type="stringlength">
        	<param name="maxLength">200</param>
        	<param name="trim">true</param>
        	<message>必须小于200个字</message>
        </field-validator>
    </field>
	<field name="enterpriseBasicVo.published">
		<field-validator type="required">
			<message>不能为空</message>
		</field-validator>
	</field>
	<field name="enterpriseBasicVo.organ.id">
		<field-validator type="required">
			<message>不能为空</message>
		</field-validator>
	</field>
</validators>